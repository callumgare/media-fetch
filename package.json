{
  "name": "media-finder",
  "version": "0.2.4",
  "description": "Easy way to search for media content on internet",
  "main": "dist/src/MediaFinder.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "watch": "npm run build -- --watch",
    "lint": "eslint --ext .ts,.js src",
    "lint:fix": "eslint --fix --ext .ts,.js src",
    "lint:prettier": "prettier --write .",
    "test": "env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "npm run test -- --watch",
    "release": "release-it"
  },
  "keywords": [
    "media"
  ],
  "author": "Callum Gare <callum@gare.id>",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "cheerio": "^1.0.0-rc.11",
    "giphy-api": "^2.0.2",
    "mime-types": "^2.1.28",
    "node-fetch": "^3.3.1",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.0"
  },
  "devDependencies": {
    "@types/giphy-api": "^2.0.3",
    "@types/jest": "^29.5.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-zod": "^1.4.0",
    "jest": "^29.5.0",
    "prettier": "2.7.1",
    "release-it": "^14.14.2",
    "ts-jest": "^29.1.0",
    "ts-jest-resolver": "^2.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}

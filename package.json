{
  "name": "media-finder",
  "version": "0.3.3",
  "description": "Easy way to search for media content on internet",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc && tsc-alias",
    "watch": "npm run build -- --watch",
    "lint": "eslint --ext .ts,.js src",
    "lint:fix": "eslint --fix --ext .ts,.js src",
    "lint:prettier": "prettier --write .",
    "test": "npm run test-jest && npm run test-packaging",
    "test-jest": "env NODE_OPTIONS=--experimental-vm-modules jest",
    "test-packaging": "cd test/packaging && npm install && tsx index.ts",
    "test:watch": "npm run test-jest -- --watch",
    "release": "release-it"
  },
  "keywords": [
    "media"
  ],
  "author": "Callum Gare <callum@gare.au>",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "crawlee": "^3.10.0",
    "deepmerge": "^4.3.1",
    "entities": "^4.5.0",
    "giphy-api": "^2.0.2",
    "mime-types": "^2.1.28",
    "node-fetch": "^3.3.1",
    "pluralize": "^8.0.0",
    "zod": "^3.21.4",
    "zod-validation-error": "^3.2.0"
  },
  "devDependencies": {
    "@types/giphy-api": "^2.0.3",
    "@types/jest": "^29.5.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-zod": "^1.4.0",
    "jest": "^29.5.0",
    "prettier": "2.7.1",
    "release-it": "^16.1.3",
    "ts-jest": "^29.1.0",
    "ts-jest-resolver": "^2.0.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.4.5"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
